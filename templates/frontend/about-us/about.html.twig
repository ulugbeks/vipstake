{# Template: About. #}

{% extends 'frontend/layout/base.html.twig' %}

{% block header %}
    {% include 'frontend/layout/headers/default.html.twig' %}
{% endblock %}

{% block body %}
    <main>
        <div class="first__screen-bg">
            <picture class="first__screen-bg-img  ">
                <source media="(min-width: 1728px)" srcset="
              /img/aboutUs/hero/1728_aboutUsHero@1.png 1x,
              /img/aboutUs/hero/1728_aboutUsHero@2.png 2x,
              /img/aboutUs/hero/1728_aboutUsHero@3.png 3x
            ">
                <source media="(min-width: 1512px)" srcset="
              /img/aboutUs/hero/1512_aboutUsHero@1.png 1x,
              /img/aboutUs/hero/1512_aboutUsHero@2.png 2x,
              /img/aboutUs/hero/1512_aboutUsHero@3.png 3x
            ">
                <source media="(min-width: 1280px)" srcset="
              /img/aboutUs/hero/1280_aboutUsHero@1.png 1x,
              /img/aboutUs/hero/1280_aboutUsHero@2.png 2x,
             /img/aboutUs/hero/1280_aboutUsHero@3.png 3x
            ">
                <source media="(min-width: 1180px)"
                        srcset="/img/aboutUs/hero/1180_aboutUsHero@1.png 1x,/img/aboutUs/hero/1180_aboutUsHero@2.png 2x,/img/aboutUs/hero/1180_aboutUsHero@3.png 3x">
                <source media="(min-width: 834px)"
                        srcset="/img/aboutUs/hero/834_aboutUsHero@1.png 1x,/img/aboutUs/hero/834_aboutUsHero@2.png 2x,/img/aboutUs/hero/834_aboutUsHero@3.png 3x">
                <source media="(min-width: 375px)"
                        srcset="/img/aboutUs/hero/375_aboutUsHero@1.png 1x,/img/aboutUs/hero/375_aboutUsHero@2.png 2x,/img/aboutUs/hero/375_aboutUsHero@3.png 3x">
                <img class="hero__slide-img " src="/img/hero/red375.png"
                     srcset="/img/aboutUs/hero/375_aboutUsHero@1.png 1x,/img/aboutUs/hero/375_aboutUsHero@2.png 2x,/img/aboutUs/hero/375_aboutUsHero@3.png 3x"
                     alt="first image">
            </picture>
            <section class="about-us--hero">
                <div id="hero" class="container about-us--hero__container">
                    <picture class="about-us--hero--symbol">
                        <source media="(min-width: 1728px)"
                                srcset="/img/aboutUs/hero/375_symbol@1.png 1x, /img/aboutUs/hero/375_symbol@2.png 2x,/img/aboutUs/hero/375_symbol@3.png 3x">
                        <source media="(min-width: 1512px)"
                                srcset="/img/aboutUs/hero/1512_symbol@1.png 1x, /img/aboutUs/hero/1512_symbol@2.png 2x,/img/aboutUs/hero/1512_symbol@3.png 3x">
                        <source media="(min-width: 1280px)"
                                srcset="/img/aboutUs/hero/1280_symbol@1.png 1x, /img/aboutUs/hero/1280_symbol@2.png 2x,/img/aboutUs/hero/1280_symbol@3.png 3x">
                        <source media="(min-width: 1180px)"
                                srcset="/img/aboutUs/hero/1180_symbol@1.png 1x, /img/aboutUs/hero/1180_symbol@2.png 2x,/img/aboutUs/hero/1180_symbol@3.png 3x">
                        <source media="(min-width: 834px)"
                                srcset="/img/aboutUs/hero/834_symbol@1.png 1x, /img/aboutUs/hero/834_symbol@2.png 2x,/img/aboutUs/hero/834_symbol@3.png 3x">
                        <source media="(min-width: 375px)"
                                srcset="/img/aboutUs/hero/375_symbol@1.png 1x, /img/aboutUs/hero/375_symbol@2.png 2x,/img/aboutUs/hero/375_symbol@3.png 3x">
                        <img class="styled-gallery-top--phone"
                             srcset="/img/aboutUs/hero/375_symbol@1.png 1x, /img/aboutUs/hero/375_symbol@2.png 2x,/img/aboutUs/hero/375_symbol@3.png 3x"
                             src="/img/aboutUs/hero/375_symbol@1.png" alt="first image" width="343">
                    </picture>
                    <h1 class="about-us--hero--title">{{ page.h1 ?? page.title }}</h1>
                    <p class="about-us--hero--desc">
                        {{ fields.main.u_h1 | raw }}
                    </p>
                </div>
            </section>
            <section class="our-mission container">
                <h2 class="our-mission--title">{{ fields.our_mission.h }}</h2>
                <p class="our-mission--desc">{{ fields.our_mission.t }}</p>
                <ul class="our-mission--list">
                    {% for block in fields.our_mission.b1 %}
                        <li class="our-mission--item">
                            <h3 class="our-mission--item__title">
					<span class="our-mission--item--icon">
                        <img src="{{ block.i }}" alt="">
				</span>{{ block.c }}
                            </h3>
                            <p class="our-mission--item--desc">{{ block.t }}</p>
                        </li>
                    {% endfor %}
                </ul>
            </section>
        </div>
        <section class="benefits-about">
            <ul class="benefits-about--list">
                {% for stat in fields.stats %}
                    <li class="benefits-about--item">
                        <p class="benefits-about--item--title">{{ stat.sc }}</p>
                        <p class="benefits-about--item--desc">{{ stat.ct }}</p>
                    </li>
                {% endfor %}
            </ul>
        </section>
        <section class="we-are container">
            {% set labelColors = ['red', 'purple', 'orange', 'green'] %}
            {{ dump(fields) }}
            <h2 class="we-are--title">{{ fields.wwa.h }}</h2>
            <div class="we-are--top">
                <div class="we-are--top--text-box">
                    <p class="we-are--top--desc">
                        {{ fields.wwa.t | raw }}
                    </p>
                </div>
                <div class="we-are--centre">
                    <p class="we-are--centre--title">Our strengths</p>
                    <ul class="we-are--centre__buttons">
                        {% for line in fields.wwa.s %}
                            <li class="button {{ labelColors[loop.index0] }}"><span>{{ line.st }}</span></li>
                        {% endfor %}
                    </ul>
                </div>
            </div>

